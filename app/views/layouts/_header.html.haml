= link_to "spacerent", root_path, id: "logo"

%nav
  - if signed_in?
    %ul
      %li#username= current_user.name
      %li= link_to "settings", edit_person_path(current_user)
      - if current_user.admin?
        = render "layouts/admin_panel_link"
      %li= link_to "logout", logout_path, method: :delete
  - else
    = render "sessions/login"
